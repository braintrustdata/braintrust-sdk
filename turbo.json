{
  "$schema": "https://turbo.build/schema.json",
  "globalPassThroughEnv": ["OPENAI_API_KEY", "ANTHROPIC_API_KEY"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"],
      "env": []
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "start": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "watch": {}
  }
}
