{
  "name": "@braintrust/otel",
  "version": "0.0.1",
  "description": "OpenTelemetry integration for Braintrust SDK",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/braintrustdata/braintrust-sdk.git",
    "directory": "integrations/braintrust-otel"
  },
  "homepage": "https://www.braintrust.dev/docs",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "module": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch",
    "clean": "rm -rf dist/*",
    "test": "vitest run"
  },
  "author": "Braintrust Data Inc.",
  "license": "MIT",
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/sdk-trace-base": "^2.0.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.203.0",
    "@opentelemetry/resources": "^2.0.1",
    "@opentelemetry/semantic-conventions": "^1.36.0",
    "@opentelemetry/core": "^2.0.1",
    "braintrust": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "tsup": "^8.3.5",
    "typescript": "^5.4.4",
    "vitest": "^2.1.9"
  },
  "peerDependencies": {
    "braintrust": ">=0.4.0"
  }
}

