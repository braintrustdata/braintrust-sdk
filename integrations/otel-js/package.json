  "license": "MIT",
  "devDependencies": {
    "braintrust": "workspace:*"
  },
  "dependencies": {},
  "peerDependencies": {
    "braintrust": "^0.4.0"
  },
  "peerDependenciesMeta": {
    "braintrust": {
      "optional": false
    }
  }
