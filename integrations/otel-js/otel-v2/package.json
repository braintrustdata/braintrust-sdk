{
  "name": "@braintrust/otel-v2-test",
  "version": "0.0.1-test",
  "private": true,
  "description": "Test package for @braintrust/otel with OpenTelemetry 2.x",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "module": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch",
    "clean": "rm -rf dist/*",
    "build:deps": "(cd ../../../.. && pnpm turbo run build --filter=braintrust --filter=@braintrust/otel)",
    "test": "pnpm run build:deps && vitest run"
  },
  "author": "Braintrust Data Inc.",
  "license": "MIT",
  "devDependencies": {
    "@braintrust/otel": "file:..",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/context-async-hooks": "^2.0.0",
    "@opentelemetry/core": "^2.0.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.200.0",
    "@opentelemetry/resources": "2.0.0",
    "@opentelemetry/sdk-node": "^0.200.0",
    "@opentelemetry/sdk-trace-base": "2.0.0",
    "@types/node": "^22.15.21",
    "braintrust": "file:../../../js",
    "msw": "^2.6.6",
    "tsup": "^8.5.0",
    "typescript": "5.5.4",
    "vitest": "^2.1.9"
  }
}
