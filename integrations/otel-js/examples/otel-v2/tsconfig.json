{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "lib": ["ES2022"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "downlevelIteration": true,
    "baseUrl": "."
  },
  "include": ["*.ts", "../common/*.ts"],
  "exclude": ["node_modules", "../../../js/**"]
}
