{
  "name": "@braintrust/auto-instrument",
  "version": "0.0.1",
  "description": "Automatic instrumentation for AI SDKs using ESM import hooks",
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./register": {
      "types": "./dist/register.d.ts",
      "import": "./dist/register.mjs"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch",
    "clean": "rm -rf dist",
    "test": "vitest run"
  },
  "author": "Braintrust Data Inc.",
  "license": "MIT",
  "devDependencies": {
    "braintrust": "workspace:*",
    "tsup": "^8.5.0",
    "typescript": "5.5.4",
    "vitest": "^2.1.9"
  },
  "dependencies": {
    "import-in-the-middle": "^2.0.1"
  },
  "peerDependencies": {
    "braintrust": ">=2.0.0"
  },
  "peerDependenciesMeta": {
    "@braintrust/otel": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.19.0"
  }
}
