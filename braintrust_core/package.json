      "types": "./jsdist/index.d.ts",
      "import": "./jsdist/bundle.js",
      "require": "./jsdist/bundle.cjs"
    }
  },
  "scripts": {
    "build": "tsc && tsx scripts/bundle-js.ts",
    "watch": "tsc --watch"
  },
