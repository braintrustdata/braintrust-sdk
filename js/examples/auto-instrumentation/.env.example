# Copy this file to .env and fill in your API keys

# Braintrust API key (get from https://www.braintrust.dev)
BRAINTRUST_API_KEY=

# OpenAI API key (get from https://platform.openai.com)
# Required for: openai-example.js, openai-streaming-example.js, vercel-ai-sdk-example.js
OPENAI_API_KEY=

# Anthropic API key (get from https://console.anthropic.com)
# Required for: anthropic-example.js, claude-agent-sdk-example.js
ANTHROPIC_API_KEY=

# Google GenAI API key (get from https://aistudio.google.com/apikey)
# Required for: google-genai-example.js
GOOGLE_GENAI_API_KEY=
