{
  "name": "braintrust-auto-instrumentation-examples",
  "version": "1.0.0",
  "private": true,
  "description": "Examples demonstrating Braintrust auto-instrumentation with various AI SDKs",
  "type": "module",
  "scripts": {
    "openai": "node --import @braintrust/auto-instrumentations/hook.mjs openai-example.js",
    "openai-streaming": "node --import @braintrust/auto-instrumentations/hook.mjs openai-streaming-example.js",
    "anthropic": "node --import @braintrust/auto-instrumentations/hook.mjs anthropic-example.js",
    "vercel": "node --import @braintrust/auto-instrumentations/hook.mjs vercel-ai-sdk-example.js",
    "google": "node --import @braintrust/auto-instrumentations/hook.mjs google-genai-example.js",
    "claude-agent": "node --import @braintrust/auto-instrumentations/hook.mjs claude-agent-sdk-example.js"
  },
  "dependencies": {
    "@ai-sdk/openai": "^3.0.0",
    "@anthropic-ai/claude-agent-sdk": "^0.1.0",
    "@anthropic-ai/sdk": "^0.60.0",
    "@braintrust/auto-instrumentations": "file:../../../integrations/auto-instrumentations",
    "@google/genai": "^1.25.0",
    "ai": "^6.0.0",
    "braintrust": "file:../..",
    "dotenv": "^16.4.5",
    "openai": "^4.0.0",
    "zod": "^3.25.34"
  }
}
