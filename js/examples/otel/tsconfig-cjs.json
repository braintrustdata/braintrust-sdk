{
  "$comment": "This is the CommonJS tsconfig used by .github/workflows/js-sdk-node-example.yaml",
  "compilerOptions": {
    "module": "CommonJS",
    "moduleResolution": "node",
    "target": "ES2020",
    "lib": ["ES2020"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "declaration": false,
    "sourceMap": false,
    "noEmitOnError": false,
    "strict": false,
    "noImplicitAny": false
  },
  "include": ["nodesdk_example.ts"],
  "exclude": ["node_modules"]
}
