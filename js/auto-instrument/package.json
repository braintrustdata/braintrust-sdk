{
  "name": "@braintrust/auto-instrument",
  "version": "0.1.0",
  "description": "Automatic instrumentation for Braintrust SDK using import hooks",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/braintrustdata/braintrust-sdk.git",
    "directory": "sdk/js/auto-instrument"
  },
  "homepage": "https://www.braintrust.dev/docs",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./register": {
      "types": "./dist/register.d.ts",
      "import": "./dist/register.mjs",
      "require": "./dist/register.js"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch",
    "clean": "rm -rf dist/*",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "braintrust",
    "instrumentation",
    "auto-instrument",
    "openai",
    "anthropic",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "braintrust": "workspace:*",
    "import-in-the-middle": "^2.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.19.16",
    "openai": "^6.4.0",
    "tsup": "^8.5.1",
    "typescript": "5.4.4",
    "vitest": "^2.1.9"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
