{
  "name": "@braintrust/smoke-test-shared",
  "version": "1.0.0",
  "private": true,
  "description": "Shared test utilities for Braintrust smoke tests",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "watch": "tsup --watch"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "tsup": "^8.0.0",
    "typescript": "^5.4.4"
  }
}
