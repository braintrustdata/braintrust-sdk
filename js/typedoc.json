{
  "compilerOptions": {
    "skipLibCheck": true
  },
  "tsconfig": "./tsconfig.json",
  "excludeExternals": true,
  "externalPattern": ["**/node_modules/**"],
  "exclude": [
    "**/examples/**",
    "**/smoke/**",
    "**/src/auto-instrumentations/**",
    "**/tests/auto-instrumentations/**"
  ],
  "plugin": ["typedoc-plugin-markdown"],
  "out": "../../docs/reference/sdks/ts-api",
  "githubPages": false,
  "disableSources": true,
  "excludePrivate": true,
  "hideBreadcrumbs": true,
  "excludeInternal": true,
  "theme": "markdown",
  "groupOrder": ["Functions", "*"]
}
