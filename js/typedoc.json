{
    "compilerOptions": {
      "skipLibCheck": true
    },
    "tsconfig": "./tsconfig.json",
    "excludeExternals": true,
    "excludeNotDocumented": true,
    "excludeNotDocumentedKinds": ["TypeAlias", "Variable"],
    "externalPattern": [
        "**/node_modules/**"
    ],
    "exclude": ["**/examples/**"],
    "plugin": ["typedoc-plugin-markdown", "typedoc-plugin-frontmatter", "./typedoc-frontmatter.mjs"],
    "out": "../../app/content/docs/reference/libs/nodejs",
    "publicPath": "/docs/libs/nodejs/",
    "githubPages": false,
    "disableSources": true,
    "excludePrivate": true,
    "hideBreadcrumbs": true,
    "hidePageHeader": true,
    "excludeInternal": true,
    "theme": "markdown",
    "groupOrder": ["Functions", "*"]
}
