{
  "name": "@braintrust/browser",
  "version": "0.0.1",
  "description": "Braintrust SDK for browser environments with AsyncLocalStorage polyfill",
  "type": "module",
  "module": "./dist/browser.js",
  "types": "./dist/browser.d.ts",
  "exports": {
    ".": {
      "types": "./dist/browser.d.ts",
      "import": "./dist/browser.js",
      "default": "./dist/browser.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "als-browser": "^1.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/braintrustdata/braintrust-sdk.git",
    "directory": "sdk/js-browser"
  },
  "homepage": "https://www.braintrust.dev/docs",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
